{
    "name": "actual-case-estimation",
    "version": "0.0.1",
    "description": "Modeling tool as a website for reporters and researchers that plots estimated actual cases.",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "license": "GPL-3.0",
    "scripts": {
        "start": "yarn && yarn run build && yarn run watch:hot",
        "analyze": "mkdir -p tmp && webpack-cli --profile --json --production > tmp/stats.json && webpack-bundle-analyzer --no-open tmp/stats.json ./dist",
        "build": "tsc -v && tsc",
        "clean": "rm -rf dist coverage lib tmp tsconfig-src.tsbuildinfo",
        "deploy": "yarn run package:prod && aws --profile ActualCaseEstimation s3 --region eu-west-1 sync dist s3://pandemic-estimator.net --acl public-read && aws --profile ActualCaseEstimation cloudfront create-invalidation --distribution-id E2G13VIYJ26ZKO --paths '/*'",
        "lint": "yarn run eslint --cache 'src/**/*.{js,jsx,ts,tsx}'",
        "lint:fix": "yarn run lint --fix",
        "lint:summary": "yarn lint:verbose | grep ERROR | sed -r 's/ERROR: \\((.*)\\)\\s\\/.*/\\1/' | sort | uniq -c | sort -nr",
        "lint:verbose": "yarn run lint -t verbose",
        "package": "webpack --mode development",
        "package:prod": "webpack --mode production",
        "test": "jest",
        "test:coverage": "yarn run test --coverage",
        "watch": "concurrently -p '[{name}]' -n 'TS,HMR' 'yarn run watch:src --preserveWatchOutput' 'yarn run watch:hot'",
        "watch:hot": "webpack-dev-server --mode development --hot",
        "watch:src": "tsc --watch  # --preserveWatchOutput",
        "watch:test": "yarn run test --watch"
    },
    "dependencies": {
        "@material-ui/core": "^4.9.5",
        "@material-ui/icons": "^4.9.1",
        "@material-ui/lab": "^4.0.0-alpha.46",
        "@material-ui/styles": "^4.9.0",
        "bignumber.js": "^9.0.0",
        "classnames": "^2.2.6",
        "d3": "^5.15.0",
        "history": "^4.10.1",
        "jhucsse.covid": "^1.3.2",
        "match-sorter": "^4.0.2",
        "mobx": "^5.15.4",
        "mobx-react-lite": "^1.5.2",
        "moment": "^2.24.0",
        "pako": "^1.0.11",
        "react": "^16.13.0",
        "react-dom": "^16.13.0",
        "react-mathjax2": "^0.0.2",
        "react-router-dom": "^5.1.2"
    },
    "devDependencies": {
        "@babel/core": "^7.8.6",
        "@babel/plugin-proposal-class-properties": "^7.8.3",
        "@babel/plugin-proposal-decorators": "^7.8.3",
        "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
        "@babel/plugin-proposal-optional-chaining": "^7.8.3",
        "@babel/preset-env": "^7.8.6",
        "@babel/preset-react": "^7.8.3",
        "@babel/preset-typescript": "^7.8.3",
        "@hot-loader/react-dom": "^16.12.0",
        "@types/d3": "^5.7.2",
        "@types/jest": "^25.1.3",
        "@types/react": "^16.9.23",
        "@types/react-dom": "^16.9.5",
        "@types/react-router-dom": "^5.1.3",
        "@types/webpack": "^4.41.7",
        "@types/webpack-env": "^1.15.1",
        "@typescript-eslint/eslint-plugin": "^2.24.0",
        "@typescript-eslint/parser": "^2.24.0",
        "babel-loader": "^8.0.6",
        "copy-webpack-plugin": "^5.1.1",
        "core-js": "^3.6.4",
        "css-loader": "^3.4.2",
        "dotenv": "^8.2.0",
        "eslint": "^6.8.0",
        "eslint-plugin-react": "^7.19.0",
        "file-loader": "^5.1.0",
        "fork-ts-checker-webpack-plugin": "^4.0.5",
        "html-loader": "^0.5.5",
        "html-webpack-plugin": "^3.2.0",
        "jest": "^25.1.0",
        "jest-circus": "^25.1.0",
        "react-hot-loader": "^4.12.19",
        "regenerator-runtime": "^0.13.5",
        "style-loader": "^1.1.3",
        "svg-loader": "^0.0.2",
        "ts-jest": "^25.2.1",
        "typescript": "^3.8.3",
        "typescript-tslint-plugin": "^0.5.5",
        "utility-types": "^3.10.0",
        "weak": "^1.0.1",
        "webpack": "^4.42.0",
        "webpack-bundle-analyzer": "^3.6.0",
        "webpack-cli": "^3.3.11",
        "webpack-dev-server": "^3.10.3"
    }
}
